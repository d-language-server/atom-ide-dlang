{
  "name": "ide-dlang",
  "main": "./lib/ide-dlang",
  "version": "1.16.4",
  "description": "D language support for Atom-IDE",
  "keywords": [
    "dlang",
    "d",
    "atom-ide",
    "ide",
    "ide-d"
  ],
  "repository": "https://github.com/d-language-server/atom-ide-dlang",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "activationHooks": [
    "ide-dlang:grammar-used"
  ],
  "enhancedScopes": [
    "source.d",
    "source.dpp"
  ],
  "configSchema": {
    "dubPath": {
      "title": "Path to the dub executable",
      "type": "string",
      "default": ""
    },
    "dlsPath": {
      "title": "Path to the DLS executable",
      "type": "string",
      "default": ""
    },
    "connectionType": {
      "title": "Method used to connect to DLS",
      "type": "string",
      "enum": [
        "stdio",
        "socket"
      ],
      "default": "stdio"
    },
    "init": {
      "title": "DLS initatization options",
      "type": "object",
      "properties": {
        "autoUpdate": {
          "title": "Enable auto-updating",
          "type": "boolean",
          "default": true
        },
        "preReleaseBuilds": {
          "title": "Enable pre-release updates",
          "type": "boolean",
          "default": false
        },
        "safeMode": {
          "title": "Disable processing multiple requests in parallel",
          "type": "boolean",
          "default": false
        },
        "catchErrors": {
          "title": "Catch and ignore errors (WARNING: UNSAFE)",
          "type": "boolean",
          "default": false
        },
        "logFile": {
          "title": "Path to a file to log DLS operations",
          "type": "string",
          "default": ""
        },
        "capabilities": {
          "title": "Enabled capabilities",
          "type": "object",
          "properties": {
            "hover": {
              "title": "Enable hover",
              "type": "boolean",
              "default": true
            },
            "completion": {
              "title": "Enable completion",
              "type": "boolean",
              "default": true
            },
            "definition": {
              "title": "Enable go-to-definition",
              "type": "boolean",
              "default": true
            },
            "typeDefinition": {
              "title": "Enable go-to-type-definition",
              "type": "boolean",
              "default": true
            },
            "references": {
              "title": "Enable references search",
              "type": "boolean",
              "default": true
            },
            "documentHighlight": {
              "title": "Enable symbol highlighting",
              "type": "boolean",
              "default": true
            },
            "documentSymbol": {
              "title": "Enable document symbol search",
              "type": "boolean",
              "default": true
            },
            "workspaceSymbol": {
              "title": "Enable workspace symbol search",
              "type": "boolean",
              "default": true
            },
            "codeAction": {
              "title": "Enable code actions",
              "type": "boolean",
              "default": true
            },
            "documentFormatting": {
              "title": "Enable formatting",
              "type": "boolean",
              "default": true
            },
            "documentRangeFormatting": {
              "title": "Enable range formatting",
              "type": "boolean",
              "default": true
            },
            "documentOnTypeFormatting": {
              "title": "Enable on type formatting",
              "type": "boolean",
              "default": true
            },
            "rename": {
              "title": "Enable renaming",
              "type": "boolean",
              "default": true
            }
          }
        },
        "symbol": {
          "title": "Symbol options",
          "type": "object",
          "properties": {
            "autoImports": {
              "title": "Automatically import projects and their dependencies",
              "type": "boolean",
              "default": true
            }
          }
        }
      }
    },
    "dls": {
      "title": "DLS configuration",
      "type": "object",
      "properties": {
        "symbol": {
          "title": "Symbols",
          "type": "object",
          "properties": {
            "importPaths": {
              "title": "Import paths",
              "type": "array",
              "default": []
            },
            "listLocalSymbols": {
              "title": "List local symbols",
              "type": "boolean",
              "default": false
            }
          }
        },
        "analysis": {
          "title": "Analysis",
          "type": "object",
          "properties": {
            "configFile": {
              "title": "D-Scanner config file",
              "type": "string",
              "default": "dscanner.ini"
            },
            "filePatterns": {
              "title": "Patterns for files to analyze (see https://dlang.org/phobos/std_path.html#.globMatch for pattern syntax)",
              "type": "array",
              "default": []
            }
          }
        },
        "format": {
          "title": "Formatting",
          "type": "object",
          "properties": {
            "engine": {
              "title": "Formatting engine",
              "type": "string",
              "enum": [
                "dfmt",
                "builtin"
              ],
              "default": "dfmt"
            },
            "endOfLine": {
              "title": "End of line",
              "type": "string",
              "enum": [
                "lf",
                "cr",
                "crlf"
              ],
              "default": "lf"
            },
            "insertFinalNewLine": {
              "title": "Insert final newline",
              "type": "boolean",
              "default": true
            },
            "trimTrailingWhitespace": {
              "title": "Trim trailing whitespace",
              "type": "boolean",
              "default": true
            },
            "maxLineLength": {
              "title": "Max line length",
              "type": "number",
              "default": 120
            },
            "softMaxLineLength": {
              "title": "Soft max line length",
              "type": "number",
              "default": 80
            },
            "braceStyle": {
              "title": "Brace style",
              "type": "string",
              "enum": [
                "allman",
                "otbs",
                "stroustrup"
              ],
              "default": "allman"
            },
            "spaceAfterCasts": {
              "title": "Space after casts",
              "type": "boolean",
              "default": true
            },
            "spaceAfterKeywords": {
              "title": "Space after keywords",
              "type": "boolean",
              "default": true
            },
            "spaceBeforeAAColons": {
              "title": "Space after AA colons",
              "type": "boolean",
              "default": false
            },
            "spaceBeforeFunctionParameters": {
              "title": "Space before function parameters",
              "type": "boolean",
              "default": false
            },
            "spaceBeforeSelectiveImportColons": {
              "title": "Space before selective import colons",
              "type": "boolean",
              "default": true
            },
            "alignSwitchStatements": {
              "title": "Align switch statements",
              "type": "boolean",
              "default": true
            },
            "compactLabeledStatements": {
              "title": "Compact labeled statements",
              "type": "boolean",
              "default": true
            },
            "outdentAttributes": {
              "title": "Outdent attributes",
              "type": "boolean",
              "default": true
            },
            "splitOperatorsAtLineEnd": {
              "title": "Split operators at line end",
              "type": "boolean",
              "default": false
            },
            "templateConstraintsStyle": {
              "title": "Template constraints style",
              "type": "string",
              "enum": [
                "conditionalNewlineIndent",
                "conditionalNewline",
                "alwaysNewline",
                "alwaysNewlineIndent"
              ],
              "default": "conditionalNewlineIndent"
            },
            "templateConstraintsSingleIndent": {
              "title": "Template constraints single indent",
              "type": "boolean",
              "default": false
            }
          }
        }
      }
    }
  },
  "consumedServices": {
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "builder": {
      "versions": {
        "2.0.0": "provideBuild"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.onSave": {
      "versions": {
        "0.1.0": "provideOnSaveCodeFormat"
      }
    },
    "code-format.onType": {
      "versions": {
        "0.1.0": "provideOnTypeCodeFormat"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "package-deps": [
    "build"
  ],
  "dependencies": {
    "atom-languageclient": "^0.9.9",
    "atom-package-deps": "^5.1.0",
    "bytes": "^3.1.0",
    "glob": "^7.1.4"
  },
  "devDependencies": {
    "@types/bytes": "^3.0.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^8.10.51"
  }
}
